{
  "name": "pragatiui-cli",
  "version": "0.1.4",
  "description": "CLI for managing UI components",
  "type": "module",
  "bin": {
    "pragatiui": "./dist/index.js"
  },
  "scripts": {
    "prepublish": "npm run build",
    "build": "tsup src/index.ts --format esm --dts",
    "dev": "tsup src/index.ts --format esm --watch --dts",
    "lint": "eslint \"src/**/*.ts*\"",
    "test": "echo \"No tests specified\" && exit 0"
  },
  "dependencies": {
    "axios": "^0.21.4",
    "chalk": "^4.1.2",
    "commander": "^9.4.1",
    "framer-motion": "^6.0.0",
    "inquirer": "^8.2.5",
    "ora": "^5.4.1",
    "react": "^18.0.0",
    "react-dom": "^18.0.0"
  },
  "devDependencies": {
    "@types/inquirer": "^8.2.5",
    "@types/node": "^18.11.9",
    "@types/react": "^18.0.0",
    "eslint": "^8.28.0",
    "tailwindcss": "^3.0.0",
    "tsup": "^6.5.0",
    "typescript": "^4.9.3"
  }
}
